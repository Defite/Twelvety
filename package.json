{
  "name": "twelvety",
  "version": "0.0.1",
  "description": "A pre-configured, super-fast Eleventy starter project",
  "scripts": {
    "clean": "rimraf dist",
    "build:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
    "build": "run-s clean build:*",
    "watch:eleventy": "cross-env ELEVENTY_ENV=development eleventy --watch",
    "watch": "run-s clean watch:*",
    "serve:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "serve": "run-s clean serve:*"
  },
  "repository": "https://github.com/gregives/twelvety",
  "keywords": [
    "eleventy",
    "starter",
    "project"
  ],
  "author": "Greg Ives",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "html-minifier": "^4.0.0",
    "js-beautify": "^1.11.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2"
  }
}
